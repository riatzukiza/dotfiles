[user]
	email = foamy125@gmail.com
	name = Error
[init]
	defaultBranch = main
[pull]
	rebase = false
[github]
	user = riatzukiza
[merge]
	tool = ediff
[mergetool "ediff"]
	cmd = emacs --eval \"(ediff-merge-files-with-ancestor \\\"$LOCAL\\\" \\\"$REMOTE\\\" \\\"$BASE\\\" nil \\\"$MERGED\\\")\"
	trustExitCode = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[core]
	excludesfile = /home/err/.config/git/ignore
